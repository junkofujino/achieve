<div class="container">
  <h2>ユーザー一覧</h2>
  <% @users.each do |user| %>
      <div class="container-child col-md-2 col-sm-4 col-xs-6 ">
        <%= link_to user_path(user.id) do %>
          <h4><strong><%= user.name %></strong></h4>
        <% end %>
        <p class="img1"><%= profile_img(user) %></p><br>
        <% unless user == current_user %>
          <%= render 'follow_form', user: user %>
        <% end %>
      </div>
  <% end %>
</div>
